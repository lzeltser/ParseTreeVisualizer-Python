<program>   ::= <stmt_list> "eof"
<stmt_list> ::= <stmt_list> <stmt> | <stmt>
<stmt>      ::= "identifier" ":=" <expr> | "read" "identifier" | "write" <expr>
<expr>      ::= <term> | <expr> <add_op> <term>
<term>      ::= <factor> | <term> <mult_op> <factor>
<factor>    ::= "(" <expr> ")" | "identifier" | "number"
<add_op>    ::= "+" | "-"
<mult_op>   ::= "*" | "/"